      "source": [
        "df_rentcharges_month_clean = df_rentcharges_month[~df_rentcharges_month['vg_id'].isin(deth_list.index)].copy()\n",
        "df_rentcharges_month_clean.loc[:, 'diff']  = round(df_rentcharges_month_clean['rent'] - df_rentcharges_month_clean['amount_disc'] - df_rentcharges_month_clean['amount'])\n",
        "\n",
        "#Берем данные только за последний месяц:\n",
        "df_rentcharges_month_clean_may = df_rentcharges_month_clean[(df_rentcharges_month_clean['year_'] == 2024) & (df_rentcharges_month_clean['month_'] == 5)]\n",
        "\n",
        "#Заменим значения diff меньше 10 на нули, для учета погрешности округления\n",
        "df_rentcharges_month_clean_may.loc[df_rentcharges_month_clean_may['diff'] < 10, 'diff'] = 0\n",
        "\n",
        "#Сгруппируем платежи с не полной олпатой по тарифам (rent) с расчетом количества платежей, суммы тарифов и сумм недоплаченых денег:\n",
        "tarifs_agg_all = df_rentcharges_month_clean_may.groupby('rent').agg({'amount': ['count','sum']})\n",
        "tarifs_agg_diff = df_rentcharges_month_clean_may[df_rentcharges_month_clean_may['diff'] != 0].groupby('rent').agg({'diff': ['count','sum']})\n",
        "\n",
        "#объединяем tarifs_agg_all и tarifs_agg_diff:\n",
        "tarifs_agg = tarifs_agg_all.join(tarifs_agg_diff, how='outer')\n",
        "tarifs_agg['amount'] = tarifs_agg['amount'].round(1)\n",
        "tarifs_agg.reset_index(inplace=True)\n",
        "tarifs_agg.columns = ['rent', 'count', 'amount_sum', 'diff_count', 'diff_sum']\n",
        "tarifs_agg"
      ],
